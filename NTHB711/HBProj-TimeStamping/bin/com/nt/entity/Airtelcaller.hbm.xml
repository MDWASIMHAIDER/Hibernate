<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
  	<hibernate-mapping>
  		<class name="com.nt.entity.Airtel" table="AIRTEL">
  			<id name="custId" column="CUSTID" type="int" length="5">
  				<generator class="increment"/>
  			</id>
  			<version name="updatecount" column="UPDATECOUNT"></version>
  			<property name="custName" column="CUSTNAME" type="string" length="15"/>
  			<property name="mobile" column="MOBILE" type="long" length="12"/>
  			<property name="callertune" column="CALLERTUNE" type="string"  length="13"/>
  		</class>
  	</hibernate-mapping>