<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
        <hibernate-mapping>
        	<class name="com.nt.entity.AirtelCallerTune" table="AirtelCallerTune">
        		<id  name="custId" column="CUSTID" type="int" length="5"/>
        		<property name="custName" column="CUSTNAME" type="string" length="15"/>
        		<property name="mobileNO" column="MOBILENO" type="long" length="12"/>
        		<property name="callerTune" column="CALLERTUNE" type="string" length="12"/>
   
        	</class>
        </hibernate-mapping>