<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        <hibernate-mapping>
        	<class name="com.nt.entity.StudentDetail" table="STUDENT_DETAIL">
        		<id name="sRoll" column="SROLL" length="5" type="int" ><!-- unsaved-value="107" -->
        			<!-- <generator class="increment"/> -->
        		</id>
        		<property name="sName" column="SNAME" length="15" type="string"/>
        		<property name="sAddress" column="SADDRESS" length="16" type="string"/>
        		<property name="sContact" column="SCONTACT" length="12" type="long" unique="true" not-null="true"/>
        	</class>
        </hibernate-mapping>