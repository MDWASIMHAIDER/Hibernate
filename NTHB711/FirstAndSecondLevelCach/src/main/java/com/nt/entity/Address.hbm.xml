<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        <hibernate-mapping>
        	<class name="com.nt.entity.Address" table="Address_Test">
        		<id name="aid" column="aid" length="8" type="int">
        			<generator class="increment"></generator>
        		</id>
        		<property name="houseNo" column="hno" length="10"/>
        		<property name="street" column="street" length="15"/>
        		<property name="area" column="area" length="14"/>
        		<property name="pin" column="pin" length="8"/>
        	</class>
        </hibernate-mapping>